<?xml version="1.0" encoding="UTF-8"?>
<cronentries>
  <cron>
    <url>/cron/cronjob</url>
    <description>checks the time up to the election and send a reminder email 1 day before the start of the election</description>
    <schedule>every day 00:00</schedule>
     <retry-parameters>
      <min-backoff-seconds>2.5</min-backoff-seconds>
      <max-doublings>5</max-doublings>
    </retry-parameters>
  </cron>
</cronentries>